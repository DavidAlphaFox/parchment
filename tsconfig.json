{
  "compilerOptions": {
    "module": "es6",
    "target": "es6",
    "lib": ["es2015", "dom", "dom.iterable"],
    "emitDeclarationOnly": true,
    "declaration": true,
    "declarationDir": "./dist/typings",
    "strict": true,
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "include": ["src"],
}
